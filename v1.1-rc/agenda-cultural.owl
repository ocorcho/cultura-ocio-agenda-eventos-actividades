###  http://schema.org/subEvent
schema:subEvent rdf:type owl:ObjectProperty ;
                owl:inverseOf schema:superEvent ;
                rdfs:domain schema:Event ;
                rdfs:range schema:Event ;
                rdfs:comment "An Event that is part of this event."@en ,
                             "Un Evento que forma parte de este evento."@es ;
                rdfs:label "sub event"@en ,
                           "sub evento"@es .


###  http://schema.org/superEvent
schema:superEvent rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:comment "An event that this event is a part of."@en ,
                               "Un evento del que forma parte este evento."@es ;
                  rdfs:label "super event"@en ,
                             "super evento"@es .


###  http://vocab.linkeddata.es/datosabiertos/def/cultura-ocio/agenda#direccion
esagenda:direccion rdf:type owl:ObjectProperty ;
                   rdfs:domain schema:Event ;
                   rdfs:range schema:Place ;
                   rdfs:comment "La direccion de, por ejemplo, el lugar donde se produce un evento, donde se encuentra una organización o donde tiene lugar una acción."@es ,
                                "The address of for example where the event is happening, an organization is located, or where an action takes place."@en ;
                   rdfs:label "direccion"@en ,
                              "direccion"@es .


###  http://vocab.linkeddata.es/datosabiertos/def/cultura-ocio/agenda#documentacion
esagenda:documentacion rdf:type owl:ObjectProperty ;
                       rdfs:domain esagenda:Evento ;
                       rdfs:range schema:DigitalDocument ;
                       rdfs:comment "Asociación con la documentación como actas, mociones, vídeos."@es ,
                                    "Association with documentation such as minutes, motions, videos."@en ;
                       rdfs:label "documentacion"@es ,
                                  "documentation"@en .


###  http://vocab.linkeddata.es/datosabiertos/def/cultura-ocio/agenda#tipo-evento
esagenda:tipo-evento rdf:type owl:ObjectProperty ;
                     rdfs:domain esagenda:Evento ;
                     rdfs:comment "Tipología que define a qué categoría pertenece el evento. Los tipos de evento se representan mediante una lista de conceptos."@es ,
                                  "Typology that defines to what category the event belongs. Event types are represented by a list of concepts."@en ;
                     rdfs:label "event type"@en ,
                                "tipo de evento"@es .


###  http://vocab.linkeddata.es/datosabiertos/def/cultura-ocio/agenda#ubicacion
esagenda:ubicacion rdf:type owl:ObjectProperty ;
                   rdfs:domain esagenda:Evento ;
                   rdfs:range dbo:GovernmentalAdministrativeRegion ;
                   rdfs:comment "Territorial scope of an event."@en ,
                                "Ámbito territorial de un evento."@es ;
                   rdfs:label "location"@en ,
                              "ubicacion"@es .


